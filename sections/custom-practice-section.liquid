

<div class="section custom-section">
<div class="home-blog-posts page-width section-padding">
   <div class="section-wrapper">
     <h3>{{ section.settings.description }}</h3>
   </div>
<div class="product-list">
  {% for product in collection.products %}
    <h4>{{ product.title }}</h4>
  {% else %}
    <p>No products available in this collection.</p>
    <p>Number of products: {{ collection.products.size }}</p>

  {% endfor %}
</div>

</div>
</div>

{% schema %}
{
  "name": "custom practice section",
  "tag": "section",
  "class": "section",
  "settings": [
    {
      "type": "text",
      "id": "description",
      "label": "Description"
    },
    
  ],
  "presets": [
    {
      "name": "custom practice section"
    }
  ]
}
{% endschema %}


